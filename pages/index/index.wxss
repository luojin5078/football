/**index.wxss**/
.header{
  height: 102.296rpx;
  background: #0dc441;
  width: 100%;
  position:fixed;
  top: 0;
  z-index: 9999;
}
.title-box{
  position: absolute;
  left: 50%;
  top: 50%;
  transform: translate(-50%,-50%);
  width:278.46rpx ;
  height:58.5rpx ;
  border-radius: 29.25rpx;
  border: none;
  outline: none;
}
.title-box text{
  color: #fff;
  font-size: 29.25rpx;
  display: inline-block;
  width: 50%;
  line-height: 58.5rpx;
  text-align: center;
  background: #3dd067;
}
.title-box .hot{
  background: #64d985;
  border-top-left-radius: 29.25rpx;
	border-bottom-left-radius: 29.25rpx;
}
.title-box .motion{
  border-top-right-radius:29.25rpx;
	border-bottom-right-radius:29.25rpx;
}
.nav{
  height: 79.56rpx;
  display: flex;
  justify-content: space-around;
  border: 1.77rpx solid #e5e5e5;
  position: fixed;
  top: 102.296rpx;
  left: 0;
  right: 0;
  background: rgba(255, 255, 255, 0.7);
  z-index: 9999;
}
.nav text{
   font-size: 29.25rpx;
   line-height: 79.56rpx;
   display: inline-block;
   width: 100%;
   text-align: center;
   color: #666;
}
.nav .nav-active{
  color:#0dc441;
  border-bottom: 2.34rpx solid #0dc441;
  box-sizing: border-box; 
}
.article{
  position: absolute;
  top: 181.856rpx;
  left: 0;
  right: 0;
  height:1315.76rpx;
   margin-bottom:102.296rpx;
}
.article .content-box:nth-child(even){
  float: right;
}
.article .content-box:nth-child(odd){
  float: left;
}
.content-box{
  width:367.38rpx;
  border: 1.77rpx solid #e5e5e5;
  margin-bottom: 17.7rpx;
  box-sizing: border-box;
}
.pic{
  width: 100%;
  height: 337.05rpx;
}
.pic image{
  width: 100%;
  height: 100%;
}
.pic-info{
  width: 341.64rpx;
  line-height: 81.84rpx;
   font-size: 29.25rpx;
   padding-left: 25.74rpx;
   border-top: 1.77rpx solid #e5e5e5;
}
.footer{
  display: flex;
  height: 102.296rpx;
  position: fixed;
  bottom: 0;
  justify-content: space-around;
  background: #fff;
  z-index: 9999;
  left: 0;
  right: 0;
  border-top: 1.77rpx solid #adadad;
	color: #d6d6d6;
}
.footer text{
  font-size: 29.25rpx;
}
.footer .footer-info:first-child{
  color: #0dc441;
}
.footer-photo{
  position: relative;
}
.camral{
  width: 121.68rpx;
  height:121.68rpx;
  border-radius: 50%;
  border: 1.77rpx solid #adadad;
  position: absolute;
  box-sizing: border-box;
  left: 50%;
  top: 40%;
  transform: translate(-50%,-50%);
  background: #fff;
}
.camral .iconfont{
  margin-top: 17.7rpx;
}
.footer .iconfont{
  display: block;
   font-size: 29.25rpx;
   margin-top: 17.7rpx;
}
.camral .iconfont{
  margin-top: 34.7rpx;
  font-size: 49.25rpx;
}
@font-face {
	font-family: 'iconfont';
	src: url(data:font/truetype;charset=utf-8;base64,) format('truetype');
	font-weight: 500;
	font-style: normal;
}


.iconfont {
  text-align: center;
  font-family:"iconfont" !important;
  font-style:normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

.icon-fangdajing:before { content: "\e662"; }

.icon-tuichu:before { content: "\e622"; }

.icon-1:before { content: "\e602"; }

.icon-combinedshape:before { content: "\e6b0"; }

.icon-xiangji:before { content: "\e610"; }
